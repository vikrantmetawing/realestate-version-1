
 <!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN"
    "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">

<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en" lang="en">
  <head>
  
  
	<!-- stylesheets -->
  	<link rel="stylesheet" href="/stylesheets/style.css" type="text/css" media="screen" />
  	<link rel="stylesheet" href="/stylesheets/slide.css" type="text/css" media="screen" />
	
  	
	 
    <!-- jQuery - the core -->
	<script src="/javascripts/jquery-1.3.2.min.js" type="text/javascript"></script>
	<!-- Sliding effect -->
	<script src="/javascripts/slide.js" type="text/javascript"></script>
	
  
  


  </head>
<body style="margin-top: 25px;">
<% unless flash[:notice].nil? %>
                       <div class="heading_featured" >
                               <fieldset style="height: 35px; background-color: #FF0040;color: #FFFFFF;padding-top:10px">
                                       <div align="center">
                                               <font size="5"><p class="flash-msg"> <%= flash[:notice] %> </p></font>
                                       </div>
                               </fieldset>
                       </div>
               <% end %>
			   

			   
			
			<br>
			<br>
			<br>
			<br>
	<!-- Panel -->

	<div id="panel">
		<div class="content clearfix">
		<%= form_tag("/real/contactmail/", :method=>"post") do %>
			<div class="left">
				<h1>Mail Your Requirements To Us</h1>
				
				
				 <label class="control-label">
			Name
			<span class="required">*</span>
			</label>
			<%= text_field_tag(:name,"") %><br/>
			
			<label class="control-label">
			Email
			<span class="required">*</span>
			</label>
			<%= text_field_tag(:email,"") %><br/>
			
			<label class="control-label">
			Phone
			<span class="required">*</span>
			</label>
			<%= text_field_tag(:phone,"") %><br/>
				
				
			</div>
			<div class="left">
				<!-- Login Form -->
				
			
			 
			<br>
			
			
			
			
			
			
			
			
			<label class="control-label">
			Property Type
			<span class="required">*</span>
			</label>
			<%= text_field_tag(:propertytype,"") %><br/>
			
			<label class="control-label">
			Sub Property Type
			<span class="required">*</span>
			</label>
			<%= text_field_tag(:subpropertytype,"") %><br/>
			
			<label class="control-label">
			Purpose
			<span class="required">*</span>
			</label>
			<%= text_field_tag(:purpose,"") %><br/>
			
			
			<br>
			<br>
			
			
			
			
		
			</div>
			<div class="left right">			
				<label class="control-label">
			Bed Room
			<span class="required">*</span>
			</label>
			<%= text_field_tag(:bedroom,"") %><br/>
			<label class="control-label">
			Price Limit
			<span class="required">*</span>
			</label>
			<%= text_field_tag(:price,"") %><br/>
			<label class="control-label">
			Location
			<span class="required">*</span>
			</label>
			<%= text_field_tag(:location,"") %><br/>
			
			
			<%= submit_tag("send",  :value => "send",:class=>"btn btn-success") %>		
			</div>
			<% end %>
		</div>
</div> <!-- /login -->	

	<!-- The tab on top -->	
	<div class="tab">
		<ul class="login">
			<li class="left">&nbsp;</li>
			<li>Send Your Requirements</li>
			<li class="sep">|</li>
			<li id="toggle">
				<a id="open" class="open" href="#">contact</a>
				<a id="close" style="display: none;" class="close" href="#">Close Panel</a>			
			</li>
			<li class="right">&nbsp;</li>
		</ul> 
	</div> <!-- / top -->
	
<!--panel -->

			   
			   
			   
			   
			   
			   
			   
			   
			   
			   
	<div class="row" style="height:70%; width:100%;">
	   <div style="float:left;height:100%; width:67%;padding-left:3%">
	   
	   
	    
			<%= form_tag("/real/contact/", :method=>"post") do %>
			
			 <fieldset style="height: 100%;width:100%; background-color:#BDBDBD; color: #000000; border: 2px solid black;padding-left:20px;">
			
			
		
			<tab id=t1><h1>Contact Page <small>get in touch with us</small></h1>
			<hr>
			<br>
			<br>
			<tab id=t1><h5><%=session[:c_contant]%></h5>
			<br>
			<br>
			 <label class="control-label">
			Name
			<span class="required">*</span>
			</label>
			<%= text_field_tag(:name,"") %><br/>
			
			<label class="control-label">
			Email
			<span class="required">*</span>
			</label>
			<%= text_field_tag(:email,"") %><br/>
			
			<label class="control-label">
			Phone
			<span class="required">*</span>
			</label>
			<%= text_field_tag(:phone,"") %><br/>
			
			<label class="control-label" for="textarea">
			Message
			<span class="required">*</span>
			</label>
			<div class="controls">
			<TEXTAREA NAME="message" COLS=40 ROWS=5></TEXTAREA>
			</div>
			<%= submit_tag("submit",  :value => "submit",:class=>"btn btn-primary") %>		
			
			<br>
			<br>
			
			
			
			</fieldset>
			<% end %>
			
		</div>


		
		


	
	</div>


</body>
</html>